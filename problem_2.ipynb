    "#problem 2\n",
    "from decimal import Decimal\n",
    "Input = Decimal(input(\"Input: \"))\n",
    "def convert_num(Input):\n",
    "    if Input == 0:\n",
    "        return \"0\"\n",
    "    binary = \"\"\n",
    "    while Input>0:\n",
    "        binary = str(Input % 2)+binary\n",
    "        Input //=2\n",
    "    return binary\n",
    "print(\"Output:\",convert_num(Input))"
   ]
